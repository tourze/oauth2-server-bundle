{
  "name": "tourze/oauth2-server-bundle",
  "description": "OAuth2服务端实现",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/orm": "^3.0",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\OAuth2ServerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\OAuth2ServerBundle\\Tests\\": "tests"
    }
  }
}